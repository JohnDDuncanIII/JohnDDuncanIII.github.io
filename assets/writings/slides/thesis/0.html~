	<script>
	 document.onkeydown = checkKey;
	 function checkKey(e) {
	     e = e || window.event;
	     if (e.keyCode == '38') {}
	     else if (e.keyCode == '40') {}
	     else if (e.keyCode == '37') {}
	     else if (e.keyCode == '39') { document.location.href = "1.html"; }
	 }
	</script>
